                                  .Label("Rect")
                                  .Data(20, 30, 25, 40, 35, 50)
                                  .BorderColor("var(--chart-3)")
                                  .BackgroundColor("var(--chart-3)")
                                  .PointStyle(PointStyle.Rect)
                                  .PointRadius(10)
                                  .PointHoverRadius(15);
