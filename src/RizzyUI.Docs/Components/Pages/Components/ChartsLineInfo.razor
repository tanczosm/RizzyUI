        })
    )
    .Options(o =&gt; o
        .Responsive(true)
        .Plugins(p =&gt; p
            .Legend(l =&gt; l.Position(LegendPosition.Top))
            .Title(t =&gt; t.Display(true).Text(&quot;Chart.js Line Chart&quot;))
        )
    )) /&gt;
                </RzCodeViewer>
            </section>

            <section class="my-8 py-2">
                <RzHeading Level="HeadingLevel.H2" QuickReferenceTitle="Multi Axis Line Chart" class="scroll-mt-20">Multi Axis Line Chart</RzHeading>
