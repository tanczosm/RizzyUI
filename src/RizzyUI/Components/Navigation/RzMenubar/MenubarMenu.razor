@namespace RizzyUI
@inherits RzComponent<MenubarMenu.Slots>

<CascadingValue Value="this" IsFixed="true">
    <HtmlElement Element="@EffectiveElement"
                 id="@Id"
                 class="@SlotClasses.GetBase()"
                 data-menubar-menu
                 data-menu-value="@Value"
                 data-slot="menubar-menu">
        @ChildContent
    </HtmlElement>
</CascadingValue>
